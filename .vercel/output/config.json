{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/newsletter/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/category/([^/]+?)/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/category(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/legal/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/legal/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/servicios/consultoria-nerc-cip/?$",
			"dest": "_render"
		},
		{
			"src": "^/servicios/ia-generativa/?$",
			"dest": "_render"
		},
		{
			"src": "^/servicios/integraciones-software/?$",
			"dest": "_render"
		},
		{
			"src": "^/servicios/ley-ciberseguridad-chilena/?$",
			"dest": "_render"
		},
		{
			"src": "^/sitemap\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/team/?$",
			"dest": "_render"
		},
		{
			"src": "^/team(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}